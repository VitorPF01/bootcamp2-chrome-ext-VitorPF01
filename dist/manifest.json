{
  "manifest_version": 3,
  "name": "My Test Extension",
  "version": "1.0.0",
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": "icons/icon-128.png"
  },
  "background": {
    "service_worker": "src/background/background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "src/content/content.js"
      ]
    }
  ],
  "icons": {
    "128": "icons/icon-128.png"
  }
}